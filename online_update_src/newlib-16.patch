--- newlib-1.16.0/newlib/libc/stdio/sprintf_chk.c	1970-01-01 07:00:00.000000000 +0700
+++ newlib-1.16.0-new/newlib/libc/stdio/sprintf_chk.c	2014-08-27 21:53:41.857230002 +0800
@@ -0,0 +1,21 @@
+#include <stdarg.h>
+#include <stdio.h>
+
+/*
+ * Stub implementation of __sprintf_chk adapted from glibc 2.7.  This 
+ * doesn't actually implement any buffer overflow protection.  It just makes
+ * the linker happy :)
+*/
+int
+__sprintf_chk (char *s, int flags, size_t slen, const char *format, ...)
+{
+  va_list arg;
+  int done;
+
+  va_start (arg, format);
+  done = vsprintf (s, format, arg);
+  va_end (arg);
+
+  return done;
+}
+
